{
  "name": "artiefact",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate": "npm-run-all generate:*",
    "generate:env": "node ./scripts/generate-env.js",
    "prepackager": "npm-run-all generate:*",
    "typecheck": "yarn tsc --skiplibcheck",
    "storybook": "npm-run-all storybook:*",
    "storybook:load": "rnstl",
    "storybook:run": "npm-run-all generate:* && start-storybook -p 7007 -c storybook | node node_modules/react-native/local-cli/cli.js start --projectRoot=storybook --watchFolders $PWD",
    "test:storyshots": "jest storybook --config=./jest.config.js",
    "update:storyshots": "npm run storybook:load && jest storybook --config=./jest.config.js -u",
    "start": "npm-run-all generate:* && node node_modules/react-native/local-cli/cli.js start",
    "start-local": "cross-env API_ENDPOINT=http://localhost:8000/ npm run -s start",
    "test": "jest",
    "lint": "eslint ."
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@react-native-community/geolocation": "1.4.2",
    "@react-native-community/netinfo": "4.1.3",
    "@react-navigation/core": "^3.5.1",
    "@sinonjs/fake-timers": "^6.0.1",
    "axios": "0.21.1",
    "react": "16.8.6",
    "react-native": "0.62.3",
    "react-native-blur": "3.2.2",
    "react-native-camera": "git+https://git@github.com/react-native-community/react-native-camera.git",
    "react-native-gesture-handler": "1.6.1",
    "react-native-indicator": "^1.0.0",
    "react-native-keychain": "3.1.3",
    "react-native-kinpaku-ui": "0.0.8",
    "react-native-maps": "0.25.0",
    "react-native-modal": "11.2.0",
    "react-native-permissions": "2.0.2",
    "react-native-reanimated": "1.7.1",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-spacing-system": "1.0.0",
    "react-native-vector-icons": "6.6.0",
    "react-navigation": "3.13.0",
    "react-navigation-header-buttons": "3.0.5",
    "react-redux": "6.0.1",
    "recompose": "0.30.0",
    "redux": "4.0.4",
    "redux-saga": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/runtime": "^7.5.0",
    "@react-native-community/eslint-config": "0.0.5",
    "@storybook/addon-actions": "5.2.3",
    "@storybook/addon-knobs": "5.2.3",
    "@storybook/addon-links": "5.2.3",
    "@storybook/addon-storyshots": "^5.2.3",
    "@storybook/addons": "5.2.3",
    "@storybook/react-native": "5.2.3",
    "@storybook/react-native-server": "5.2.3",
    "@types/node": "10.12.0",
    "@types/react": "16.9.34",
    "@types/react-native": "0.60.31",
    "@types/react-native-indicators": "^0.16.0",
    "@types/react-native-vector-icons": "6.4.1",
    "@types/react-redux": "7.1.1",
    "@types/recompose": "0.30.6",
    "@types/redux": "3.6.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "0.54.1",
    "npm-run-all": "4.1.5",
    "react-dom": "16.8.6",
    "react-native-storybook-loader": "1.8.0",
    "react-native-typescript-transformer": "1.2.12",
    "react-test-renderer": "16.8.6",
    "ts-jest": "24.0.0",
    "tslint": "5.13.1",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
