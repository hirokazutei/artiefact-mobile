{
  "name": "artiefact",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate": "npm-run-all generate:*",
    "generate:env": "node ./scripts/generate-env.js",
    "prepackager": "npm-run-all generate:*",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007 -c storybook",
    "start": "npm-run-all generate:* && node node_modules/react-native/local-cli/cli.js start",
    "start-local": "cross-env API_ENDPOINT=http://localhost:8000/ npm run -s start",
    "test": "jest",
    "lint": "eslint ."
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@react-native-community/geolocation": "^1.4.2",
    "@react-native-community/netinfo": "^4.1.3",
    "artiefact": "https://github.com/hirokazutei/artiefact-mobile.git",
    "axios": "^0.19.0",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-blur": "^3.2.2",
    "react-native-camera": "git+https://git@github.com/react-native-community/react-native-camera.git",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-indicator": "^1.0.0",
    "react-native-keychain": "^3.1.3",
    "react-native-kinpaku-ui": "https://github.com/hirokazutei/react-native-kinpaku-ui",
    "react-native-maps": "0.25.0",
    "react-native-modal": "^11.2.0",
    "react-native-permissions": "^2.0.2",
    "react-native-spacing-system": "0.4.2",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.11.1",
    "react-navigation-header-buttons": "^3.0.1",
    "react-redux": "^6.0.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "tslint": "^5.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react-native": "^5.2.3",
    "@storybook/react-native-server": "^5.2.3",
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.60.2",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-redux": "^7.1.1",
    "@types/recompose": "^0.30.6",
    "@types/redux": "^3.6.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-dom": "16.8.6",
    "react-native-storybook-loader": "^1.8.0",
    "react-native-typescript-transformer": "^1.2.12",
    "react-test-renderer": "16.8.6",
    "ts-jest": "^24.0.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
